{
  "name": "rtm-cli",
  "version": "1.5.1",
  "description": "RTM CLI",
  "keywords": [
    "rtm",
    "cli",
    "terminal",
    "remember the milk",
    "tasks",
    "task manager",
    "todo",
    "gtd"
  ],
  "bin": {
    "rtm": "src/cli.js"
  },
  "main": "src/utils/index.js",
  "scripts": {
    "postversion": "npm run build",
    "build": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwaring87/rtm-cli.git"
  },
  "author": "David Waring <dev@davidwaring.net> (https://davidwaring.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dwaring87/rtm-cli/issues"
  },
  "homepage": "https://github.com/dwaring87/rtm-cli#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "cli-table3": "^0.6.0",
    "commander": "^10.0.0",
    "copy-paste": "^1.3.0",
    "dateformat": "^5.0.0",
    "deepmerge": "^2.0.1",
    "opn": "^5.4.0",
    "ora": "^5.0.0",
    "prompt-sync": "^4.2.0",
    "rtm-api": "github:beauraines/rtm-api",
    "window-size": "^1.1.0"
  },
  "devDependencies": {
    "archiver": "^2.1.0",
    "jest": "^29.2.2",
    "pkg": "^4.3.3"
  },
  "pkg": {
    "scripts": [
      "config.json",
      "src/cmd/*.js"
    ],
    "targets": "node8-linux-x86,node8-linux-x64,node8-macos-x86,node8-macos-x64,node8-win-x86,node8-win-x64",
    "out": "./bin"
  }
}
